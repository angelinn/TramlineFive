             xmlns:local="clr-namespace:TramlineFive.Pages;assembly=TramlineFive"
             x:Class="TramlineFive.Pages.MasterDetail"
             MasterBehavior="Popover">

    <MasterDetailPage.Master>
        <ContentPage Title="wut" Padding="0,40,0,0">
            <Label Text="lol wut" />
        </ContentPage>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
